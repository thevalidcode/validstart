{
  "name": "validstart",
  "version": "1.0.9",
  "description": "ValidStart is a powerful and intuitive command-line interface (CLI) tool meticulously crafted to streamline the project setup process.",
  "main": "dist/index.js",
  "bin": {
    "validstart": "dist/index.js"
  },
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "lint": "eslint src --fix",
    "release": "standard-version && git push --follow-tags"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.7.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.0.10",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [],
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ],
    "outputPath": "pkg-build"
  }
